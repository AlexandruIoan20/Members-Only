doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        link(rel='stylesheet', href='/stylesheets/style.css')
        title Document
    body 
    block content 
        header  
            h1 View all members 

        main 
            ul 
                for user in users 
                    li
                        p #{user.username} 
                        if user !== reqUser
                            p 
                                a(href= `${user.url}/promote`) Promote